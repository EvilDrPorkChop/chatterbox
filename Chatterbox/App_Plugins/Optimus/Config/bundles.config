<?xml version="1.0" encoding="utf-8"?>
<optimus>
  <settings>
    <setting key="onlyAllowCssFilesInEditor" value="True" />
    <setting key="onlyAllowJsFilesInEditor" value="True" />
    <setting key="compileDynamicStyleSheetsToCss" value="True" />
    <setting key="compileDynamicJavaScriptToJs" value="True" />
    <setting key="ignoreCompilationDebug" value="True" />
  </settings>
  <bundles>
    <styleBundle virtualPath="~/bundles/MainCSS" disableMinification="False">
      <include virtualPath="~/css/nicdark_style.css" />
      <include virtualPath="~/css/nicdark_responsive.css" />
      <include virtualPath="~/css/fontello.css" />
      <include virtualPath="~/css/nicdark_menu.css" />
      <include virtualPath="~/css/magnific-popup.css" />
      <include virtualPath="~/css/nicdark_shortcodes.css" />
      <include virtualPath="~/css/animate.css" />
    </styleBundle>
    <scriptBundle virtualPath="~/bundles/MainScripts" disableMinification="False">
      <include virtualPath="~/scripts/main/excanvas.js" />
      <include virtualPath="~/scripts/main/html5.js" />
      <include virtualPath="~/scripts/main/jquery-ui.js" />
      <include virtualPath="~/scripts/main/jquery.min.js" />
    </scriptBundle>
    <scriptBundle virtualPath="~/bundles/RevSlider" disableMinification="False">
      <include virtualPath="~/scripts/plugins/revslider/jquery.themepunch.revolution.min.js" />
      <include virtualPath="~/scripts/plugins/revslider/jquery.themepunch.tools.min.js" />
    </scriptBundle>
    <scriptBundle virtualPath="~/bundles/Nav" disableMinification="False">
      <include virtualPath="~/scripts/plugins/navigation/hoverIntent.js" />
      <include virtualPath="~/scripts/plugins/navigation/superfish.min.js" />
      <include virtualPath="~/scripts/plugins/navigation/tinynav.min.js" />
    </scriptBundle>
    <scriptBundle virtualPath="~/bundles/Menu" disableMinification="False">
      <include virtualPath="~/scripts/plugins/menu/superfish.min.js" />
      <include virtualPath="~/scripts/plugins/menu/tinynav.min.js" />
    </scriptBundle>
    <scriptBundle virtualPath="~/bundles/Other" disableMinification="False">
      <include virtualPath="~/scripts/plugins/isotope/isotope.pkgd.min.js" />
      <include virtualPath="~/scripts/plugins/mpopup/jquery.magnific-popup.min.js" />
      <include virtualPath="~/scripts/plugins/scroolto/scroolto.js" />
      <include virtualPath="~/scripts/plugins/nicescrool/jquery.nicescroll.min.js" />
      <include virtualPath="~/scripts/plugins/inview/jquery.inview.min.js" />
      <include virtualPath="~/scripts/plugins/parallax/jquery.parallax-1.1.3.js" />
      <include virtualPath="~/scripts/plugins/countto/jquery.countTo.js" />
      <include virtualPath="~/scripts/plugins/countdown/jquery.countdown.js" />
    </scriptBundle>
    <scriptBundle virtualPath="~/bundles/Settings" disableMinification="False">
      <include virtualPath="~/scripts/settings.js" />
    </scriptBundle>
    <scriptBundle virtualPath="~/bundles/JQuery" disableMinification="False">
      <include virtualPath="~/scripts/jquery-3.1.1.js" />
      <include virtualPath="~/scripts/main/jquery.min.js" />
      <include virtualPath="~/scripts/jquery-3.1.1.min.js" />
      <include virtualPath="~/scripts/main/jquery-ui.js" />
      <include virtualPath="~/scripts/main/excanvas.js" />
    </scriptBundle>
    <scriptBundle virtualPath="~/bundles/master" disableMinification="False">
      <include virtualPath="~/scripts/jquery-3.1.1.js" />
      <include virtualPath="~/scripts/main/jquery.min.js" />
      <include virtualPath="~/scripts/main/jquery-ui.js" />
      <include virtualPath="~/scripts/main/excanvas.js" />
      <include virtualPath="~/scripts/main/html5.js" />
      <include virtualPath="~/scripts/plugins/countdown/jquery.countdown.js" />
      <include virtualPath="~/scripts/plugins/countto/jquery.countTo.js" />
      <include virtualPath="~/scripts/plugins/inview/jquery.inview.min.js" />
      <include virtualPath="~/scripts/plugins/isotope/isotope.pkgd.min.js" />
      <include virtualPath="~/scripts/plugins/menu/superfish.min.js" />
      <include virtualPath="~/scripts/plugins/menu/tinynav.min.js" />
      <include virtualPath="~/scripts/plugins/mpopup/jquery.magnific-popup.min.js" />
      <include virtualPath="~/scripts/plugins/navigation/hoverIntent.js" />
      <include virtualPath="~/scripts/plugins/navigation/superfish.min.js" />
      <include virtualPath="~/scripts/plugins/navigation/tinynav.min.js" />
      <include virtualPath="~/scripts/plugins/nicescrool/jquery.nicescroll.min.js" />
      <include virtualPath="~/scripts/plugins/parallax/jquery.parallax-1.1.3.js" />
      <include virtualPath="~/scripts/plugins/revslider/jquery.themepunch.revolution.min.js" />
      <include virtualPath="~/scripts/plugins/revslider/jquery.themepunch.tools.min.js" />
      <include virtualPath="~/scripts/plugins/scroolto/scroolto.js" />
      <include virtualPath="~/scripts/CustomSlider.js" />
    </scriptBundle>
  </bundles>
</optimus>